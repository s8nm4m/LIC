add wave -position end  sim:/keyscan/KScan
add wave -position end  sim:/keyscan/I0
add wave -position end  sim:/keyscan/I1
add wave -position end  sim:/keyscan/I2
add wave -position end  sim:/keyscan/I3
add wave -position end  sim:/keyscan/CLK
add wave -position end  sim:/keyscan/Reset
add wave -position end  sim:/keyscan/Kpress
add wave -position end  sim:/keyscan/O0
add wave -position end  sim:/keyscan/O1
add wave -position end  sim:/keyscan/O2
add wave -position end  sim:/keyscan/K
force -freeze sim:/keyscan/KScan 0 0
force -freeze sim:/keyscan/I0 1 0
force -freeze sim:/keyscan/I1 1 0
force -freeze sim:/keyscan/I2 1 0
force -freeze sim:/keyscan/I3 1 0
force -freeze sim:/keyscan/CLK 1 0, 0 {50 ps} -r 100
force -freeze sim:/keyscan/Reset 0 0
run
force -freeze sim:/keyscan/Reset 1 0
run
