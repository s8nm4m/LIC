# Generic modules to activate from Simulator
UsbPort = UsbPort
kbd = Keyboard("123456789*0#",4,3,0)
lcd = LCD

# --------------------------------------
# KeyBoard Reader
# --------------------------------------
1 -> kbd.oe
kbd.K[0-3] -> UsbPort.I[1-4]
kbd.val -> UsbPort.I0
UsbPort.O0 -> kbd.ack

# --------------------------------------
# LCD
# --------------------------------------
UsbPort.O[3-6] -> lcd.D[4-7]
UsbPort.O2 -> lcd.rs
UsbPort.O1 -> lcd.e
